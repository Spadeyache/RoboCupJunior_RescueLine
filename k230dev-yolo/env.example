# Environment Variables for K230D YOLOv8n Pipeline
# Copy this file to .env and adjust values as needed

# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Docker Configuration
COMPOSE_PROJECT_NAME=k230d-yolo

# Training Configuration
DEFAULT_EPOCHS=100
DEFAULT_BATCH_SIZE=16
DEFAULT_IMG_SIZE=320

# Paths (relative to docker-compose.yml)
WORKSPACE_PATH=./workspace
DATASETS_PATH=./datasets
MODELS_PATH=./models
RUNS_PATH=./runs
OUTPUT_PATH=./output

# Model Configuration
MODEL_INPUT_SIZE=320
CONFIDENCE_THRESHOLD=0.25
IOU_THRESHOLD=0.45

# K230D Target Configuration
TARGET_DEVICE=k230
QUANTIZATION_TYPE=uint8

# nncase Configuration
NNCASE_VERSION=2.10.0
